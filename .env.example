# MongoDB Configuration
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_here

# Admin Authentication
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD=admin_password

# Server Configuration
PORT=5001
NODE_ENV=development

# Voice Integration API
VOICE_API_BASE_URL=https://your-voice-api-url.com
VOICE_API_KEY=your_voice_api_key

# Dify Chatbot API
DIFY_API_BASE_URL=https://your-dify-api-url.com/v1
DIFY_API_KEY=your_dify_api_key

# Frontend Environment Variables
VITE_VOICE_API_KEY=your_voice_api_key
VITE_DIFY_API_KEY=your_dify_api_key
VITE_VOICE_API_BASE_URL=https://your-voice-api-url.com
VITE_DIFY_API_BASE_URL=https://your-dify-api-url.com/v1
